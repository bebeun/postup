<% (user.class.name == "User") ? (name = user.name) : (name = Profile::PROFILE_SYMBOL[user.profile.class.name]+" : "+user.profile.description) %>
<% if user_signed_in? %>
	<% if current_user == user %>
		<% name = "You" %>
	<% end %>
<% end %>
<%= (name == "You") ? (name) : (link_to(name, user)) %>


