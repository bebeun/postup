<!DOCTYPE html>
<html>
	<%= render 'layouts/header' %>
	
	<body>
		<%= render 'layouts/navbar' %>

	    <% flash.each do |message_type, message| %>
			<div class="alert alert-<%= message_type %>"><%= message %></div>
		<% end %>

		<%= yield %>

		<%= javascript_include_tag 'application' %>
		<%= yield(:after_js) %>
		
	</body>
	
</html>
